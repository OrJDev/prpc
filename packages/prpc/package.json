{
  "name": "prpc",
  "description": "Package for easily creating server rpc functions in solid-start with goodies baked in",
  "version": "0.0.1",
  "scripts": {
    "build": "tsup --config ../../tsup.config.ts",
    "clean": "rm -rf ./node_modules dist .turbo .solid",
    "dev": "tsup --config ../../tsup.config.ts --watch",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "exports": {
    "worker": {
      "solid": "./dist/server.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": "./dist/server.cjs"
    },
    "browser": {
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": "./dist/dev.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "deno": {
      "solid": "./dist/server.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": "./dist/server.cjs"
    },
    "node": {
      "solid": "./dist/server.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": "./dist/server.cjs"
    },
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "main": "./dist/server.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@types/node": "^18.7.14",
    "babel-preset-solid": "^1.5.3",
    "rollup": "^2.79.0",
    "solid-js": "^1.5.3",
    "typescript": "^4.8.2",
    "solid-start": "^0.2.22",
    "@adeora/solid-query": "0.19.0"
  },
  "dependencies": {
    "@tanstack/query-core": "https://pkg.csb.dev/TanStack/query/commit/bc5a2a16/@tanstack/query-core",
    "@adeora/solid-query": "^0.19.0"
  },
  "peerDependencies": {
    "solid-start": "^0.2.22",
    "solid-js": "^1.5.3",
    "zod": "^3.20.6"
  },
  "engines": {
    "node": ">=16"
  },
  "typesVersions": {},
  "module": "./dist/server.js"
}
