{
  "name": "@prpc/react",
  "description": "Package for easily creating server rpc functions in Bling with goodies baked in",
  "version": "0.2.26",
  "scripts": {
    "build": "rm -rf dist && tsup --config ./tsup.config.js",
    "clean": "rm -rf ./node_modules dist .turbo",
    "dev": "rm -rf dist && tsup --config ./tsup.config.js --watch",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "exports": {
    "worker": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "browser": {
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "node": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "browser": {
    "./dist/index.js": "./dist/index.js",
    "./dist/index.cjs": "./dist/index.cjs"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.26.1",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^5.44.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "typescript": "^4.8.2",
    "zod": "^3.20.6"
  },
  "dependencies": {
    "@prpc/core": "workspace:*"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.26.1",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "zod": "^3.20.6"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "typesVersions": {},
  "module": "./dist/index.js"
}
